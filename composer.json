    "require": {
        "php": "^7.3",
        "laravel/lumen-framework": "^8.2",
        "ext-json": "^7.4"
    },
    "autoload": {
        "classmap": [
            "database/migrations"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    }
}
